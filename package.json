    "db:migrate:local": "npx wrangler d1 execute omefans-db --file=./0001_add_userid_to_gallery.sql"
  },
  "dependencies": {
    "hono": "^4.4.6"
  },
  "devDependencies": {
    "typescript": "^5.4.5",
    "wrangler": "^3.60.3"
  }
}
